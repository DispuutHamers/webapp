<h1>Groepen</h1>

  <% @usergroups.each do |group| %>
    <li>
      <%= group.name %>
    </li>
  <% end %>
</ul>

<% if current_user.admin? %>
<%= form_for(@group) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= f.text_field :name %>
  <%= f.submit "Create", class: "btn btn-large btn-primary" %>
<% end %>
<% end %>
